var int x;
var int y;
var int z;
var bool flag;

main {
  // Tests initiaux
  x = 1;
  y = 6;
  x = x + 2;       // Mise à jour simple
  y = y * (x + 4); // Utilisation de la variable x dans une expression
  print(y);

  // Ajout de tests supplémentaires
  z = 10;
  print(z);

  x = x * 2;       // Multiplication
  print(x);

  y = y / x;       // Division
  print(y);

  flag = (x == 8); // Comparaison et affectation à une variable booléenne
  print(flag);

  if (flag) {
    z = z + 5;     // Modification conditionnelle
  } else {
    z = z + 10;
  }
  print(z);

  x = (x + y) % z; // Combinaison de plusieurs opérations
  print(x);
}
